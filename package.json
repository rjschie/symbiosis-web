{
  "name": "symbiosis",
  "version": "0.1.0",
  "description": "The Symbiosis companion app.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "yarn lint:es && yarn lint:types",
    "lint:es": "eslint . --ext=js,ts --cache",
    "lint:types": "tsc --noEmit true",
    "pretty": "yarn prettier --write \"**/*.{ts,js,css}\"",
    "start": "webpack-dev-server",
    "test": "testem ci",
    "test:watch": "testem"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@glimmer/babel-plugin-glimmer-env": "~2.0.0-beta.5",
    "@glimmer/babel-plugin-strict-template-precompile": "~2.0.0-beta.5",
    "@glimmer/blueprint": "~2.0.0-beta.5",
    "@glimmer/component": "~2.0.0-beta.5",
    "@glimmer/core": "~2.0.0-beta.5",
    "@glimmer/env": "^0.1.7",
    "@glimmer/helper": "~2.0.0-beta.5",
    "@glimmer/modifier": "~2.0.0-beta.5",
    "@glimmer/tracking": "^1.0.0",
    "@types/qunit": "^2.9.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^6.0.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "^4.0.4",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-sorting": "^5.0.1",
    "precss": "^4.0.0",
    "prettier": "^2.0.2",
    "qunit": "^2.9.3",
    "qunit-dom": "^1.1.0",
    "style-loader": "^1.1.3",
    "tailwindcss": "^1.2.0",
    "testem": "^3.0.3",
    "typescript": "~3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "browserslist": {
    "production": [
      "last 2 Edge versions",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "engines": {
    "node": ">= 12.0"
  },
  "private": true
}
